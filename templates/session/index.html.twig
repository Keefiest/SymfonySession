{% extends 'base.html.twig' %}

{% block title %}Hello SessionController!{% endblock %}

{% block body %}
<h3>Registre des sessions</h3>
{% for session in sessions %}
    <a class="btn btn-sm btn-outline-light" href="{{ path('show_session', {'id': session.id}) }}">{{ session }}</a></br>
{% endfor %}
<p>
    <a class="btn btn-sm btn-outline-dark" href="{{path('add_session')}}">Ajouter une session</a>
</p>
{% endblock %}
