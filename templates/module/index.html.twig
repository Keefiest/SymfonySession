{% extends 'base.html.twig' %}

{% block title %}Registre des modules{% endblock %}

{% block body %}
<h3>Registre des modules</h3>
{% for module in modules %}
    <a class="btn btn-sm btn-outline-light" href="{{path('show_module', {'id': module.id })}}">{{ module.moduleName }}</a><br>
{% endfor %}
{% endblock %}
